{
  "name": "PHP & MySQL Dev Environment",
  "dockerComposeFile": [
    "compose.yaml"
  ],
  "service": "app",
  "workspaceFolder": "/var/www/html",
  "customizations": {
    "vscode": {
      "extensions": [
        "bmewburn.vscode-intelephense-client",
        "xdebug.php-debug"
      ],
      "settings": {
        "php.validate.executablePath": "/usr/local/bin/php"
      }
    }
  },
  "remoteUser": "vscode"
}
